{
  "name": "Central Bank Tracker",
  "description": "Tracker for updates on central bank",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "48": "/assets/image/favicon48x48.png",
    "72": "/assets/image/favicon72x72.png",
    "144": "/assets/image/favicon144x144.png",
    "192": "/assets/image/favicon192x192.png"
  },
  "background": {
    "scripts": ["./background.js"]
  },
  "options_page": "./pages/options.html",
  "browser_action": {},
  "permissions": [
    "alarms",
    "storage",
    "tabs",
    "activeTab",
    "https://www.climaaovivo.com.br/mg/belo-horizonte/regiao-central"
  ]
}
